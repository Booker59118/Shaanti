        <link rel="stylesheet" href="https://bootswatch.com/5/minty/bootstrap.min.css">
        <link rel="stylesheet" href="./css/base.css">
        
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        
        {% block stylesheets %}
           
        {% endblock %}

        {% block javascripts %}
            
        {% endblock %}
